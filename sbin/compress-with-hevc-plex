    then echo "$EXIT_CODE_DESC_DIRECTORY_NOT_FOUND"
	echo "Usage: $(basename $0)" 
	exit $EXIT_CODE_DIRECTORY_NOT_FOUND
fi

function f_cwhd () {
    if $(f_is_directory "$1"); then 
        rsync -avuE --progress "$1"/* "$2" | tee -a "$3"
        rm -r "$1"
        fi
    }

