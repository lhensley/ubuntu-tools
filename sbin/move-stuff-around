    TARGET="$CONTAINER/$SOURCE_SHOW_NAME_YEAR $SOURCE_DB_TAG/Season $TARGET_SEASON/$SOURCE_SHOW_NAME_YEAR - s""$TARGET_SEASON""e""$TARGET_EPISODE - $EPISODE_TITLE - rerun.mp4"
    TARGETX="$CONTAINER/$SOURCE_SHOW_NAME_YEAR $SOURCE_DB_TAG/Season $TARGET_SEASON/$SOURCE_SHOW_NAME_YEAR - s""$TARGET_SEASON""e""$TARGET_EPISODE - $EPISODE_TITLE.mp4"
    if test -f "$TARGETX" ; then rm -f "$SOURCE" ; else TARGET_DIR=$(dirname "$TARGET") && mkdir -p "$TARGET_DIR" && if test -f "$SOURCE" ; then mv "$SOURCE" "$TARGET" ; fi; fi
